<h1>New Subtopic</h1>


<%= form_tag(subtopics_url, method: 'post') do %>


  <div>
    <%= label_tag :title %><br />
    <%= text_field_tag :title %>
  </div>

  <div>
    <%= label_tag :topic_id %><br />
    <%= number_field_tag :topic_id %>
  </div>

  <div>
    <%= submit_tag "Create Subtopic" %>
  </div>
<% end %>
